<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.kadkada">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                />
            </intent-filter>
        </activity>
        <activity android:name="com.katkada.Activity.AboutUsActivity" />
        <activity android:name="com.katkada.Activity.PrivacyPolicyActivity" />
        <activity
            android:name="com.katkada.Activity.Login"
            android:theme="@style/AppTheme.Dark">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name="com.katkada.Activity.Register" />
        <activity android:name="com.katkada.Activity.ListviewcheckboxExample"></activity>
    </application>

</manifest>